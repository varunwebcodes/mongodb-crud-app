<% layout("/layouts/boilerplate.ejs") %>
<body>

<h3>All Listings</h3>
<form action="/listings/new" method="GET">
    <button>
    Create New Listing
    </button>   
</form>
<ul>
    <% allListings.forEach(listing => { %>
        <li>
            <a href="/listings/<%= listing._id %>">
                <%= listing.title %>
            </a>
        </li>
    <% }) %>
</ul>

</body>
</html>
